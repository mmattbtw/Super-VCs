// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("REAL_DB")
}

model Server {
  id String @id @unique

  channelId String? @default("")
  signedUpUsers String[] @default([])
}
